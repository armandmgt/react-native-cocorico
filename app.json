{
  "expo": {
    "name": "Cocorico",
    "slug": "cocorico",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "app.cocorico",
      "softwareKeyboardLayoutMode": "pan"
    },
    "ios": {
      "bundleIdentifier": "app.cocorico",
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Un clone de tinder pour les poules et les coqs avec cam√©ra, gesture de swipe, localisation et messagerie.",
    "githubUrl": "https://github.com/armandmgt/react-native-cocorico",
    "privacy": "unlisted"
  }
}
